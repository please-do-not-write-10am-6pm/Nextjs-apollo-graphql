{
  "name": "nextjs-apollo-graphql",
  "version": "1.0.0",
  "scripts": {
    "dev": "export NODE_ENV='development' ; node index.js",
    "clean": "rm -rf .next",
    "build": "npm run clean && deploy/local.sh",
    "build:next": "export NODE_ENV='production' ; npx next build",
    "start": "npm run start:docker:app && npm run start:docker:static  ",
    "start:docker:app": "docker run -d -p 4000:80 --env-file ./deploy/local.env --name example-app example-app",
    "start:docker:static": "docker run -d -p 5000:80 --env-file ./deploy/local.env --name example-static example-static",
    "stop": "npm run stop:docker:static && npm run stop:docker:app",
    "stop:docker:static": "docker stop example-static && docker rm example-static",
    "stop:docker:app": "docker stop example-app && docker rm example-app",
    "start:prod": "node index.js",
    "deploy": "deploy/now.sh"
  },
  "dependencies": {
    "@weco/next-plugin-transpile-modules": "0.0.2",
    "@zeit/next-sass": "0.2.0",
    "express": "4.16.3",
    "isomorphic-unfetch": "2.0.0",
    "next": "6.1.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "apollo-boost": "0.1.10",
    "carbon-components": "9.1.10",
    "carbon-components-react": "6.7.4",
    "carbon-icons": "7.0.7",
    "graphql": "0.13.2",
    "node-sass": "4.9.0",
    "react-apollo": "2.1.9"
  },
  "devDependencies": {
    "dotenv": "6.0.0"
  }
}
